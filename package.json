{
  "name": "pyjscomp-compiler",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf lib/compiler",
    "conpiler:build": "npm run clean && tsc --outDir lib/compiler",
    "conpiler:test": "nyc mocha",
    "conpiler:watch": "mocha-typescript-watch"
  },
  "main": "index.js",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "@types/chai": "^4.1.3",
    "@types/chai-arrays": "^1.0.2",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^5.2.0",
    "@types/node": "^8.0.57",
    "@types/object-hash": "^1.2.0",
    "@types/sequelize": "^4.27.16",
    "@types/type-detect": "^4.0.1",
    "chai": "^4.1.2",
    "chai-arrays": "^2.0.0",
    "chai-exclude": "^1.0.8",
    "mocha": "^5.1.1",
    "mocha-each": "^1.1.0",
    "mocha-typescript": "^1.1.12",
    "nyc": "^11.4.1",
    "object-hash": "^1.3.0",
    "source-map-support": "^0.5.0",
    "ts-loader": "^4.2.0",
    "type-detect": "^4.0.8",
    "lodash": "^4.17.10",
    "vis": "^4.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  }
}
